@model ResetPasswordModelView
@{
    ViewData["Title"] = "RestPassword";
    Layout = "~/Views/Shared/_Outh.cshtml";
}

<div class="main-w3layouts wrapper">
	<h1>Creative SignUp Form</h1>
	<div class="main-agileinfo">
		<div class="agileits-top">
			<form action="RestPassword" method="post">
				<input class="text" asp-for="NewPassword" placeholder="Password" required="">
				<input class="text w3lpass" asp-for="ConfirmedPassword" placeholder="Confirm Password" required="">
				
				<input type="submit" value="SIGNUP">
			</form>
		</div>
	</div>

	@section  ClintSideValidation{

		<partial name="_ValidationScriptsPartial" />
	}



